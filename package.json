{
    "name": "@murphye/apollo-yarn-action-docker-example",
    "version": "0.0.1",
    "private": true,
    "scripts": {
      "dev": "nodemon",
      "lint": "eslint \"src/**/*.ts\"",
      "build": "yarn clean && esbuild src/index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --external:@inigolabs/ref-napi --external:@inigolabs/ffi-napi --external:inigo.js --external:inigo-darwin-arm64 --external:inigo-darwin-amdm64 --external:inigo-linux-amd64 --external:inigo-linux-arm64 --external:graphql && mkdir dist/node_modules && cp -R node_modules/debug node_modules/node-gyp-build node_modules/ref-struct-di node_modules/ms node_modules/inigo* node_modules/@inigolabs node_modules/graphql node_modules/graphql-request dist/node_modules/",
      "generate": "yarn generate:codegen && yarn generate:custom",
      "generate:codegen": "graphql-codegen --verbose --config codegen.yml",
      "generate:custom": "ts-node src/scripts/generate/index.ts",
      "generate:check": "yarn generate",
      "graphql-schema:generate": "node scripts/generate-graphql-schema.js",
      "graphql-schema:verify": "graphql-inspector diff current.schema.graphql new.schema.graphql",
      "lint:fix": "eslint \"src/**/*.ts\" --fix",
      "format": "run-p _format:*",
      "start": "NODE_PATH=./dist node dist/index.js",
      "clean": "shx rm -rf ./dist",
      "tsc": "tsc --noEmit --skipLibCheck --esModuleInterop --p tsconfig.json",
      "lint-test-typecheck": "run-p lint test tsc",
      "_format:prettier": "prettier --write .",
      "_reset:env": "shx rm -rf ./.env",
      "_reset:build": "yarn clean",
      "_reset:node_modules": "shx rm -rf ./node_modules"
    },
    "dependencies": {
      "apollo-server": "3.12.1",
      "axios": "1.6.2",
      "graphql": "16.8.1",
      "inigo-darwin-arm64": "0.30.1",
      "inigo-darwin-amd64": "0.30.1",
      "inigo-linux-arm64": "0.30.1",
      "inigo-linux-amd64": "0.30.1",
      "inigo.js": "0.30.1"
    },
    "devDependencies": {
      "@graphql-codegen/add": "4.0.1",
      "@graphql-codegen/cli": "3.0.0",
      "@graphql-codegen/typescript": "3.0.0",
      "@graphql-codegen/typescript-resolvers": "3.0.0",
      "@graphql-eslint/eslint-plugin": "3.12.0",
      "@graphql-inspector/cli": "3.4.0",
      "@types/jest": "29.1.2",
      "@types/lodash": "4.14.186",
      "@types/md5": "2.3.2",
      "@types/node": "16.18.23",
      "@types/node-fetch": "2.6.2",
      "@typescript-eslint/eslint-plugin": "5.40.1",
      "@typescript-eslint/parser": "5.40.1",
      "shx": "0.3.4",
      "esbuild": "0.15.12",
      "eslint": "8.37.0",
      "nodemon": "3.0.1",
      "ts-node": "10.9.1",
      "tsconfig-paths": "4.1.0",
      "typescript": "4.8.4"
    }
  }